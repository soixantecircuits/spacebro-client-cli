  "contributors": [
    "4quet",
    "hugohil (http://hugohil.tech/)",
    "gabrielstuff (http://soixantecircuits.fr)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soixantecircuits/spacebro-client-cli/issues"
  },
  "engines": {
    "node": ">=v6.3.0"
  },
  "homepage": "https://github.com/soixantecircuits/spacebro-client-cli#readme",
  "dependencies": {
    "spacebro-client": "^0.3.4",
    "standard-settings": "^1.1.7",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "sleep-promise": "^2.0.0"
