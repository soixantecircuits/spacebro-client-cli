{
  "name": "clibro",
  "version": "1.2.2",
  "description": "Command Line Interface for spacebro-client",
  "main": "index.js",
  "bin": {
    "spacebro-client": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "eslint --ext .js tests index.js",
    "test": "ava tests/*.js --verbose --timeout=2s",
